{
  "name": "liveprinter-core",
  "version": "1.0.5",
  "description": "Programmatic 3D printing library",
  "type" : "module",
  "files": ["dist"],
  "main" : "./dist/liveprinter.umd.cjs",
  "module":"./dist/liveprinter.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
