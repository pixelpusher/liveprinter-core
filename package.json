  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/pixelpusher/liveprinter-core/issues"
  },
  "homepage": "https://github.com/pixelpusher/liveprinter-core#readme",
  "devDependencies": {
    "vite": "^4.3.9",
    "liveprinter-utils": "github:pixelpusher/lp-utils",
    "tonal": "^2.2.2"
  }
}
